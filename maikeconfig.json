                "libext_format":"-l^",
                "libint_format":"-l:^",
                "mode":"c++",
                "objcompile":{
                    "args":[
                        "-c",
                        "-g",
                        "-fpic",
                        "{cxxversion}",
                        "-Wall",
                        "{cflags_extra}",
                        "{iquote}",
                        "{includedir}",
                        "-DMAIKE_TARGET_DIRECTORY={target_directory}",
                        "-DMAIKE_CURRENT_DIRECTORY={current_directory}",
                        "{includes_extra}",
                        "-o",
                        "{target}",
                        "{source}"
                    ],
                    "name":"g++"
                },
                "pkgconfig":{
                    "args":[
                        "{action}",
                        "{libname}"
                    ],
                    "name":"pkg-config"
                },
                "stdprefix":"-std=",
                "versionquery":{
                    "args":[
                        "-E",
                        "-dM",
                        "-x",
                        "c++",
                        "{nullfile}"
                    ],
                    "name":"g++"
                }
            },
            "filename_exts":[
                ".c++",
                ".cc",
                ".cpp",
                ".cxx",
                ".h",
                ".h++",
                ".hh",
                ".hpp",
                ".hxx"
            ],
            "name":"targetcxx_default",
            "plugin":"targetcxx"
        },
        {
            "config":{},
