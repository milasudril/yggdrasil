                }
            },
            "filename_exts":[
                ".py"
            ],
            "name":"targetpython_default",
            "plugin":"targetpython"
        },
        {
            "config":{
                "tar":{
                    "command":{
                        "args":[
                            "{target_strip}",
                            "{root_append}",
                            "{compressor}",
                            "-cf",
                            "{target}",
                            "-T",
                            "-"
                        ],
                        "name":"tar"
                    },
                    "compressors":{
                        "bzip2":"-j",
                        "gzip":"-z",
                        "lzma":"-J"
                    },
                    "root_append":"--transform=s,\\\\(.*\\\\),^\\\\1,g",
                    "target_strip":"--transform=s,\\^^,,g"
                },
                "zip":{
                    "create":{
                        "args":[
                            "-@",
                            "{target}"
                        ],
                        "name":"zip"
                    },
                    "rename":{
                        "args":[
                            "-w",
                            "{target}"
                        ],
                        "name":"zipnote"
                    }
                }
            },
            "filename_exts":[
                ".archive"
            ],
            "name":"targetarchive_default",
            "plugin":"targetarchive"
        },
        {
            "config":{
                "command":{
                    "args":[
                        "{script}",
                        "{args}"
                    ],
                    "name":"bash"
                }
            },
            "filename_exts":[
                ".sh"
            ],
            "name":"targetbash_default",
            "plugin":"targetbash"
        },
        {
            "config":{
                "filter":{
                    "args":[
